        "gpgkey": "https://download.fedoraproject.org/pub/epel/RPM-GPG-KEY-EPEL-16",
        "failovermethod": "priority",
        "gpgcheck": true,
        "enabled": true,
        "managed": true,
        "make_cache": true
      }
    },
    "openvpn": {
      "root_group": "root",
      "fs_prefix": "",
      "configure_default_server": true,
      "git_package": false,
      "key": {
        "ca_expire": 3650,
        "expire": 3650,
        "size": 2048,
        "country": "US",
        "province": "CA",
        "city": "San Francisco",
        "org": "Fort Funston",
        "email": "admin@foobar.com",
        "message_digest": "sha256"
      },
      "client_prefix": "vpn-prod",
      "key_dir": "/etc/openvpn/keys",
      "signing_ca_key": "/etc/openvpn/keys/ca.key",
      "signing_ca_cert": "/etc/openvpn/keys/ca.crt",
      "user_query": "*:*",
      "cookbook_user_conf": "openvpn",
      "user_databag": "users",
      "type": "server",
      "subnet": "10.8.0.0",
      "netmask": "255.255.0.0",
      "gateway": "vpn.",
      "client_cn": "client",
      "server_verification": null,
      "push_routes": [

      ],
      "push_options": [

      ],
      "config": {
        "user": "nobody",
        "group": "nogroup",
        "proto": "udp",
        "port": "1194",
        "keepalive": "10 120",
        "log": "/var/log/openvpn.log",
        "push": null,
        "script-security": 2,
        "up": "/etc/openvpn/server.up.sh",
        "persist-key": "",
        "persist-tun": "",
        "comp-lzo": "",
        "ca": "/etc/openvpn/keys/ca.crt",
        "key": "/etc/openvpn/keys/server.key",
        "cert": "/etc/openvpn/keys/server.crt",
        "dh": "/etc/openvpn/keys/dh2048.pem",
        "crl-verify": "/etc/openvpn/crl.pem",
        "server": "10.8.0.0 255.255.0.0",
        "dev": "tun0"
